{
	"name": "vm_to_cosmos",
	"properties": {
		"activities": [
			{
				"name": "Do the copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MongoDbV2Source",
						"batchSize": 100
					},
					"sink": {
						"type": "CosmosDbMongoDbApiSink",
						"writeBatchTimeout": "00:30:00",
						"writeBatchSize": 10,
						"writeBehavior": "insert"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "executions",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "cosmos_executions",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}